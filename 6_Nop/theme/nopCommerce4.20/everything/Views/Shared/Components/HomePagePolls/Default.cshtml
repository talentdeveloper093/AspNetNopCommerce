@model IList<PollModel>
@if (Model.Count > 0)
{
  <div class="home-page-polls pt-5 pb-5">
     <div class="container">
        <div class="title mid-content-title">
            <h2>@T("Polls.Title")</h2>
        </div>
        @foreach (var poll in Model)
        { 
            @await Html.PartialAsync("_Poll", poll)
        }
    </div>
  </div>
}
