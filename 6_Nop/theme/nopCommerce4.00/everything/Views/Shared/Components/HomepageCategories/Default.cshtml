@model IList<CategoryModel>
@if (Model.Count > 0)
{
  <div class="container">  
    <div class="home-page-category-grid category">
        <ul>
        @foreach (var item in Model)
        {
                <li class="col-xs-12 col-sm-3 col-md-3">
                <div class="item-box text-center">
                    <div class="category-item offer home-page-categories">
                        <h2 class="product-title pull-text-center">
                            <a href="@Url.RouteUrl("Category", new { SeName = item.SeName })" title="@item.PictureModel.Title"> @item.Name</a>
                        </h2>
                        <div class="picture">
                            <a href="@Url.RouteUrl("Category", new { SeName = item.SeName })" title="@item.PictureModel.Title">
                            <img alt="@item.PictureModel.AlternateText" src="@item.PictureModel.ImageUrl" title="@item.PictureModel.Title" /></a>
                        </div>
                    </div>
                </div>
            </li>
        }
       </ul>
    </div>
 </div>
}